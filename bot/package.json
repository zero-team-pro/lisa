{
  "name": "lisa",
  "version": "0.0.1",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-bot-gateway": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/gateway.js",
    "start-bot-cluster": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/cluster.js",
    "start-bot-telegram": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/telegram.js",
    "migrate": "sequelize-cli db:migrate --config src/configs/sequelize.js --migrations-path src/migrations",
    "start-gateway": "run-s migrate start-bot-gateway",
    "start-cluster": "run-s start-bot-cluster",
    "start-telegram": "run-s start-bot-telegram",
    "mig-gen": "npx sequelize-cli migration:generate --migrations-path src/migrations --name",
    "db:makemigrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js --migrations-path src/migrations --models-path dist/models"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zero-team-pro/lisa.git"
  },
  "bugs": {
    "url": "https://github.com/zero-team-pro/lisa/issues"
  },
  "homepage": "https://github.com/zero-team-pro/lisa#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.183.0",
    "@discordjs/rest": "^1.2.0",
    "@node-redis/json": "^1.0.2",
    "amqplib": "^0.10.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "discord.js": "^14.5.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "i18next": "^21.9.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.6.7",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.23.2",
    "sequelize-auto-migrations": "github:scimonster/sequelize-auto-migrations#a063aa6535a3f580623581bf866cef2d609531ba",
    "sequelize-typescript": "^2.1.3",
    "slate": "^0.82.1",
    "telegraf": "^4.10.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.14",
