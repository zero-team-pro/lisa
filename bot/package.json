    "@discordjs/rest": "^1.2.0",
    "@node-redis/client": "^1.0.6",
    "@node-redis/json": "^1.0.2",
    "amqplib": "^0.10.4",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "discord.js": "^14.15.3",
    "dotenv": "^16.1.3",
    "express": "^4.19.2",
    "got": "^11.8.6",
    "i18next": "^21.9.2",
    "lodash": "^4.17.21",
    "mastercard-oauth1-signer": "^1.1.6",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.7",
    "openai": "^4.52.2",
