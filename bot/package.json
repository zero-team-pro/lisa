{
  "name": "lisa",
  "version": "0.0.1",
  "license": "MIT",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.js",
    "test-ci": "CI=1 jest --config=jest.config.js",
    "start-bot-gateway": "node --loader ./src/loader.js --experimental-modules --import=extensionless/register dist/gateway.js",
    "start-bot-cluster": "node --loader ./src/loader.js --experimental-modules --import=extensionless/register dist/cluster.js",
    "start-bot-telegram": "node --loader ./src/loader.js --experimental-modules --import=extensionless/register dist/telegram.js",
    "start-bot-vm": "node --loader ./src/loader.js --experimental-modules --import=extensionless/register dist/vm.js",
    "migrate": "sequelize-cli db:migrate --config src/configs/sequelize.cjs --migrations-path src/migrations",
    "migrate-undo": "sequelize-cli db:migrate:undo --config src/configs/sequelize.cjs --migrations-path src/migrations",
    "start-gateway": "run-s migrate start-bot-gateway",
    "start-cluster": "run-s start-bot-cluster",
    "start-telegram": "run-s start-bot-telegram",
    "start-vm": "run-s start-bot-vm",
    "mig-gen": "npx sequelize-cli migration:generate --migrations-path src/migrations --name",
    "db:makemigrations": "node  --loader ./src/loader.js --experimental-modules --import=extensionless/register ./node_modules/sequelize-auto-migrations/bin/makemigration.mjs --migrations-path src/migrations --models-path dist/models"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zero-team-pro/lisa.git"
  },
  "bugs": {
    "url": "https://github.com/zero-team-pro/lisa/issues"
  },
  "homepage": "https://github.com/zero-team-pro/lisa#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "3.344.0",
    "@discordjs/rest": "1.2.0",
    "@node-redis/client": "1.0.6",
    "@node-redis/json": "1.0.2",
    "amqplib": "0.10.4",
    "axios": "1.4.0",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cron": "3.1.7",
    "discord.js": "14.15.3",
    "dockerode": "4.0.2",
    "dotenv": "16.1.3",
    "express": "4.19.2",
    "extensionless": "1.9.9",
    "got": "14.4.1",
    "i18next": "21.9.2",
    "js-tiktoken": "1.0.12",
    "lodash": "4.17.21",
    "mastercard-oauth1-signer": "1.1.6",
    "mime-types": "2.1.35",
    "module-alias": "2.2.2",
    "node-fetch": "2.6.7",
    "openai": "5.12.1",
    "p-map": "6.0.0",
    "pg": "8.11.0",
    "pg-hstore": "2.3.4",
    "prom-client": "15.1.3",
    "redis": "4.6.14",
    "reflect-metadata": "0.1.13",
    "remark-parse": "11.0.0",
    "remark-stringify": "11.0.0",
    "sequelize": "6.37.3",
    "sequelize-typescript": "2.1.5",
    "slate": "0.82.1",
    "telegraf": "4.16.3",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "url-value-parser": "2.2.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/amqplib": "0.10.5",
    "@types/dockerode": "3.3.29",
    "@types/express": "4.17.21",
    "@types/mime-types": "2.1.1",
    "@types/node": "18.19.39",
    "@types/sequelize": "4.28.14",
    "@types/uuid": "10.0.0",
    "@types/validator": "13.7.7",
    "@zeroteam/sequelize-auto-migrations": "1.1.0",
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "jest-teamcity": "^1.12.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.4.2",
    "prettier-plugin-sql": "0.18.1",
    "pretty-quick": "3.1.3",
    "sequelize-cli": "6.5.1",
    "ts-jest": "^29.4.1"
  }
}
